<?xml version="1.0" encoding="UTF-8"?>
<providers xmlns="http://fofancy.com/schema/providers-config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://fofancy.com/schema/providers-config providers-config-schema.xsd">
    <provider>
        <provider-name>Wiki</provider-name>

        <services-implementations>
            <service-implementation-info>
                <service-name>geographical-objects-info</service-name>
                <info>
                    <wsdl-url>http://localhost:8180/wiki-geographical-objects/geographical-objects-info?wsdl</wsdl-url>
                    <namespace>http://wiki.fofancy.com/</namespace>
                    <port-name>WikiGeographicalObjectsInfoReceiverServiceEndpointPort</port-name>
                </info>
            </service-implementation-info>
            <service-implementation-info>
                <service-name>nearest-objects</service-name>
                <info>
                    <wsdl-url>http://localhost:8180/wiki-map-info-receiver/nearest-objects?wsdl</wsdl-url>
                    <namespace>http://wiki.fofancy.com/</namespace>
                    <port-name>WikiNearestObjectsReceiverServiceEndpointPort</port-name>
                </info>
            </service-implementation-info>
        </services-implementations>
    </provider>
</providers>
